            // calculate hours
            const hours = Math.floor(diffInMilliSeconds / 3600) % 24;
            diffInMilliSeconds -= hours * 3600;

            // calculate minutes
            const minutes = Math.floor(diffInMilliSeconds / 60) % 60;
            diffInMilliSeconds -= minutes * 60;

            let difference = '';
            if (days > 0) {
                difference += (days === 1) ? `${days} day, ` : `${days} days, `;
            }
            if (hours > 0) {
                difference += (hours === 1) ? `${hours} hour, ` : `${hours} hours, `;
            }

            difference += (minutes === 0 || hours === 1) ? `${minutes} minutes` : `${minutes} minutes`; 

            return difference;
        }

        ////Setup (Runs on inital creation of webpage)////
        const sent = new Date({{created}});
        const expiry = new Date({{exp}});
        document.getElementById("sent").innerText = `${sent.toDateString()} at ${sent.getHours()}:${sent.getMinutes()}`;
        document.getElementById("exp").innerText = timeDiffCalc(expiry, Date.now());
        
        //Hide/show wget
        if (!{{restrict_wget}}) {
            document.getElementById("wget").innerText = `wget ${window.location.href}?force`;
        } else {
            document.getElementById("wget_container").classList.add("hide");
        }

        ////Listeners and Handlers////
        document.getElementById("button").onclick = () => {
            if (Date.now() < expiry) window.location.href = `${window.location.href}?force`;
        }

        document.getElementById("copy_clipboard").onclick = () => {
            if (Date.now() < expiry) {
                const el = document.createElement('textarea');
                el.value = `wget ${window.location.href}?force`;
                document.body.appendChild(el);
                el.select();
                document.execCommand('copy');
                document.body.removeChild(el);
            }
        }

        //Disable scrolling via keys
        window.addEventListener("keydown", function(e) {
        // space, page up, page down and arrow keys:
            if([32, 33, 34, 37, 38, 39, 40].indexOf(e.keyCode) > -1) {
                e.preventDefault();
            }
        }, false);

        setTimeout(() => {
            let status = document.getElementById("status");
            status.innerText="expired";
            status.style.color = "red";
            document.getElementById("exp").style.color = "red";
            document.getElementById("button").classList.add("expired");
            document.getElementById("wget_container").classList.add("hide");
        }, Math.max(0, expiry - Date.now()));

    </script>
