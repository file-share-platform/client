<!-- Author: Josiah Bull -->
<!-- The basic download page that someone will be shown when they click on a download link. -->

<!DOCTYPE html>
<head>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
    <meta content="utf-8" http-equiv="encoding">
    <title>Download File</title>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link href="https://fonts.googleapis.com/css2?family=Roboto&display=swap" rel="stylesheet"> 
    <style>
        body {
            overflow: hidden;
            font-family: 'Roboto', sans-serif;
            background-color: #ededed;
        }
        
        h1 {
            text-align: center;
            margin: 10px;
            margin-left: 20px;
            
        }

        .download {
            width: 60%;
            padding: 20px;
            position: fixed;
            top: 40%;
            left: 50%;
            margin-left: -30%;
            margin-top: -5%;
            box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
            transition: 0.3s;
            border-radius: 3px;
            background-color: #f0f0f0;
        }
        /* .download:hover {
            box-shadow: 0 10px 16px 0 rgba(0,0,0,0.3);
        } */

        .button {
            border-radius: 15px;
            color: white;
            background-color:#3f51b5;
            font-weight: bold;
            cursor: pointer;
        }

        .download_button {
            width: 300px;
            margin: 20px auto 10px auto;
        }

        .button:hover {
            background-color:#303f9f;
            box-shadow: 0 4px 8px 0 rgba(0,0,0,0.2);
        }

        .divider {
            height: 1px;
            width: 75%;
            padding: 0px;
            margin:  0px 0px 0px 15%;
            background-color: grey;
        }
        .svg_download_icon {
            margin: auto;
        }

        .details {
            margin: 20px 15% 20px;
            padding: 2%;
            width: 71%; 
            background-color: rgb(223, 223, 223);
            box-shadow: 0 0 5px rgb(94, 94, 94) inset; 
            border-radius: 3px;
            /* border-style: solid; */
            /* border-color: #3f51b5; */
            /* border-width: 1px; */
            color: #212121;
            overflow: hidden;
        }
        .details .code {
            font-size: 1.2em;
            letter-spacing: 0.05em;
            line-height: 1.5em;
            margin: 0px;
        }

        .expired {
            background-color: darkgray;
            opacity: 0.5;
            color: rgb(83, 83, 83);
            cursor:not-allowed;
            pointer-events: none;
        }

        .hide {
            display:none;
        }

        .copy_clipboard:hover {
            background-color: darkgray;
            cursor: pointer;
            border-radius: 3px;
        }

        .copy_clipboard:active {
            background-color: #303f9f;
        }

    </style>

</head>
